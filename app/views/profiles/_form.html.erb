<%= form_for @profile do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :last_name %>
  <%= f.text_field :last_name %>

  <%= f.label :birth_date %>
  <%= f.date_select :birth_date %>

  <%= f.label :biography %>
  <%= f.text_area :biography %>

  <%= f.label :country %>
  <%= f.text_field :country %>

  <%= f.label :city %>
  <%= f.text_field :city %>

  <%= f.label :phone_number %>
  <%= f.text_field :phone_number %>

  <%= f.label :cell_phone %>
  <%= f.text_field :cell_phone %>

  <%= f.label :blood_type %>
  <%= f.text_field :blood_type %>

  <%= f.submit %>

<% end %>
