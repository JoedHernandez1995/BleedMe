<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Last Name</th>
      <th>Birth Date</th>
      <th>Bio</th>
      <th>Country</th>
      <th>City</th>
      <th>Phone</th>
      <th>Cellphone</th>
      <th>Blood Type</th>
    </tr>
  </thead>
  <% @profiles.each do |profile| %>
  <tbody>
    <td><%=profile.name%></td>
    <td><%=profile.last_name%></td>
    <td><%=profile.birth_date%></td>
    <td><%=profile.biography%></td>
    <td><%=profile.country%></td>
    <td><%=profile.city%></td>
    <td><%=profile.phone_number%></td>
    <td><%=profile.cell_phone%></td>
    <td><%=profile.blood_type%></td>
  </tbody>
  <% end %>
</table>
